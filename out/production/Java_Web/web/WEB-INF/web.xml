<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.beluga.web.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
        <servlet-name>TestRegisterServlet</servlet-name>
        <servlet-class>com.beluga.servlets.TestRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestRegisterServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TestLoginServlet</servlet-name>
        <servlet-class>com.beluga.servlets.TestLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestLoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TestResponseIOServlet</servlet-name>
        <servlet-class>com.beluga.servlets.TestResponseIOServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestResponseIOServlet</servlet-name>
        <url-pattern>/responseIOServlet</url-pattern>
    </servlet-mapping>
</web-app>